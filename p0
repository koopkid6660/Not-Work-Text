        local user2=cmouse.ReadText:InvokeClient(playa, twuser)
        local user3=cmouse.ReadText:InvokeClient(playa, threewayuser)
        for i,v in pairs(SearchPlayer(user)) do
                for c,b in pairs(SearchPlayer(user2)) do
                        for y,x in pairs(SearchPlayer(user3)) do
                        coroutine.resume(coroutine.create(function()
                        p1 = v.Name
                        p2 = b.Name
                        p3 = x.Name
                        repeat wait()
                        until workspace[p1] and workspace[p2] and workspace[p3]
                        t1 = workspace[p1].HumanoidRootPart
                        t2 = workspace[p2].HumanoidRootPart
                        t3 = workspace[p3].HumanoidRootPart
