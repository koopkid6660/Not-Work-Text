local user=cmouse.ReadText:InvokeClient(playa, usertest)
for i,v in pairs(SearchPlayer(user)) do
coroutine.resume(coroutine.create(function()
local char=v.Character
local t=char.HumanoidRootPart
char.Head.face.Texture = nkdfc
n=t.Neck
local lw=Instance.new('Weld',t)
lw.Name='leftWeld'
