mouse.KD.OnServerEvent:connect(function(plr, key)
    if string.byte(key) == 52 then
        char.Humanoid.WalkSpeed = 21
    end
end)
mouse.KU.OnServerEvent:connect(function(plr, key)
    if string.byte(key) == 52 then
        char.Humanoid.WalkSpeed = 5
    end
end)
