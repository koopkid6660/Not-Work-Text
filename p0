	['Scripts'] = {
		{'Button', 1, '', ref = 'Drop'};
		{'Dropdown', 'Drop', UDim2.new(0, 0, 0, 0), UDim2.new(1, 0, 1, 0), {'All'}, ref = 'Dropdown'};
		function(hook) -- [[ Dropdown Control ]]
			hook.cval = 'All'
			function fixPlayerDrop()
				local t = {'All'}
				for i, v in pairs(game.Players:GetPlayers()) do
					table.insert(t, v.Name)
				end
				Dropdown.SetTable(t)
			end			
			game.Players.PlayerAdded:connect(function()
				fixPlayerDrop()
			end)
			game.Players.PlayerRemoving:connect(function()
				fixPlayerDrop()
			end)
			--event.Event:Connect(updatePlayer)
			Dropdown.Changed(function(cval) hook.cval = cval end)
			Dropdown.Select(PlayerName)
			fixPlayerDrop()
		end;
		{'TextBox', 1, '', ref = 'Search'};
		function(hook) -- Script search control
			local origY = hook.drawY
			local function MakeList(condition)
				for i,v in pairs(hook.main:GetChildren()) do
					if v.Name == "Script" then
						v:Destroy()
					end
				end
				hook.drawY = origY
				local children = TK.Scripts
				table.sort(children, function ( a, b )
					return a:lower() < b:lower()
				end)
				for i, v in pairs(children) do
					if string.find(v:lower(), condition:lower()) or (condition == "") or (condition == " ") then
						local scr = TK.GUI.Button(hook, 1, v, 25, nil, true)
						scr.MouseButton1Down:Connect(function()
							TK.Action:FireServer("Scripts", "Script", v, hook.cval)
						end)
						scr.Name = 'Script'
					end
				end
			end
			game:GetService("UserInputService").InputChanged:Connect(function(inp)
				if inp.UserInputType == Enum.UserInputType.TextInput then
					if Search:IsFocused() then
						MakeList(Search.Text)
					end
				end
			end)
			game:GetService("UserInputService").InputEnded:Connect(function(inp)
				if inp.UserInputType == Enum.UserInputType.Keyboard then
					if Search:IsFocused() then
						MakeList(Search.Text)
					end
				end
			end)
			MakeList('')
		end,
		
	},
