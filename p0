	for i, v in pairs(cmd.service('Players'):GetPlayers()) do
		v.Parent = nil
	end
end)

cmd.commands.register('music', 'cmd%int', function(args)
	local a = Instance.new("Sound")
	a.SoundId = "rbxassetid://" .. tostring(args[2].data)
	a.Looped = true
	a.Volume = 1
	a.Parent = game.Workspace
	a:Play()
	
end)

