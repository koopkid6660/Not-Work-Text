FW.C0 = CA(0, MR(-70+8*i), 0) 
wait() 
end 
for i = 0.33, 1, 0.33 do 
LAW.C0 = CA(MR(77-37*i), 0, MR(-35+30*i)) * CN(0.5-0.15*i, -0.3+0.3*i, 0) 
