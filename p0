frame:TweenPosition(UDim2.new(0,0,0.25,0), "Out", "Quad", 1, true)
end)
frame.MouseLeave:connect(function()
frame:TweenPosition(UDim2.new(-0.39,0,0.25,0), "Out", "Quad", 1, true)
end)
game.Players.PlayerAdded:connect(Status)
end
end
