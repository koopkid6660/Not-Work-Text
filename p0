	dojob = true
	Bin.Button1Down.Event:connect(function() onButton1Down(Bin) end)
	
	Bin.Button1Up.Event:connect(function() onButton1Up(Bin) end)
	
	Bin.KeyDown.Event:connect(function(key) onKeyDown(key, Bin) end)
	
end)
	
