	['Settings'] = {
		{'Text', 1, 'Patch: ' .. PatchVersion};
		{'Text', 1, 'Devnote: 5.2 is now 100% client-sided!'};
		{'Spacing'};
		{'Text', 1, '    Primary Color', ref = 'primaryTxt', props = {TextXAlignment = 'Left'}};
		{'Text', 1, '    Secondary Color', ref = 'secondaryTxt', props = {TextXAlignment = 'Left'}};
		{'Text', 1, '    Tertiary Color', ref = 'tertiaryTxt', props = {TextXAlignment = 'Left'}};
		{'Text', 1, '    Text Color', ref = 'textTxt', props = {TextXAlignment = 'Left'}};
		function(hook)
			local function createColorButton(parent, which)
				local obj = TK.Util.Object("TextButton", {
					Parent = parent;
					BackgroundColor3 = TK.Settings[which]();
					BorderSizePixel = 1;
					Position = UDim2.new(1, -52, 0, 2);
					Size = UDim2.new(0, 50, 1, -4);
					Font = 'SourceSans';
					FontSize = 'Size14';
					Text = 'Change';
					TextSize = 14;
					TextColor3 = TK.Util.Shift(TK.Settings.Text(), -50);
					ZIndex = 15;
				})
				obj.MouseButton1Down:connect(function()
					TK.Data:Fire("openpicker", TK.Settings[which](), which)
				end)
				return obj
			end

			hook.primaryBtn = createColorButton(primaryTxt, "Primary");
			hook.secondaryBtn = createColorButton(secondaryTxt, "Secondary");
			hook.tertiaryBtn  = createColorButton(tertiaryTxt, "Tertiary");
			hook.textBtn  = createColorButton(textTxt, "Text");
			TK.Data.Event:connect(function(cmd, cdata, clr)
				if cmd ~= "colorpicker" then return end
				if cdata == "TagColor" or cdata == "ChatColor" or cdata == "NameColor" then
					return
				else
					TK.settings.Theme[cdata] = clr
					if cdata == "Primary" then TK.Data:Fire("settopbar", clr) end
					TK.GUI.ApplyTheme()
				end
			end)
		end;
		{'TextBox', 1/3, '', ref = 'imageBox', props = {ClipsDescendants = true}};
		function()
			imageBox.Text = TK.Settings.Image().Image ~= "" and TK.Settings.Image().Image or 'Enter Image ID'
			imageBox.FocusLost:Connect(function(e)
				if e == true then
					TK.GUIObject.Final.Main.Image.Image = "rbxassetid://" .. imageBox.Text
					TK.settings.Theme.Image.Image = tonumber(imageBox.Text)
					local image = TK.Settings.Image()
					if image.Image == "" or image.Image == nil then return end
					if image.ImageSize == 1 then
						TK.GUIObject.Final.Main.Image.Position = UDim2.new(0.319, 0,0.064, 0)
						TK.GUIObject.Final.Main.Image.Size = UDim2.new(0.681, 0,-0.21, 405)
					elseif image.ImageSize == 2 then
						TK.GUIObject.Final.Main.Image.Position = UDim2.new(0.319, 0,0, 0)
						TK.GUIObject.Final.Main.Image.Size = UDim2.new(0.681, 0,-0.012, 405)
					elseif image.ImageSize == 3 then
						TK.GUIObject.Final.Main.Image.Position = UDim2.new(0, 0,0, 0)
						TK.GUIObject.Final.Main.Image.Size = UDim2.new(1, 0,-0.012, 405)
					end
				end
			end)
		end;
		{'Button', 1/3, '', ref = 'scaleBtn', props = {ClipsDescendants = true}, click = function()
			local image = TK.Settings.Image()
			image.ImageSize = image.ImageSize + 1
			if image.ImageSize > 3 then
				image.ImageSize = 1
			end
			TK.settings["Theme"].Image.ImageSize = image.ImageSize
			if image.ImageSize == 1 then
				scaleBtn.Text = 'Fit'
				TK.GUIObject.Final.Main.Image.Position = UDim2.new(0.319, 0, 0, 0)
				TK.GUIObject.Final.Main.Image.Size = UDim2.new(0.681, 0, -0.012, 405)
				TK.GUIObject.Final.Main.Image.ScaleType = Enum.ScaleType.Fit
			elseif image.ImageSize == 2 then
				scaleBtn.Text = 'Stretch'
				TK.GUIObject.Final.Main.Image.Position = UDim2.new(0.319, 0, 0, 0)
				TK.GUIObject.Final.Main.Image.Size = UDim2.new(0.681, 0, -0.012, 405)
				TK.GUIObject.Final.Main.Image.ScaleType = Enum.ScaleType.Stretch
			elseif image.ImageSize == 3 then
				scaleBtn.Text = 'Full'
				TK.GUIObject.Final.Main.Image.Position = UDim2.new(0, 0, 0, 0)
				TK.GUIObject.Final.Main.Image.Size = UDim2.new(1, 0, -0.012, 405)
				TK.GUIObject.Final.Main.Image.ScaleType = Enum.ScaleType.Stretch
			end
		end};
		function()
			scaleBtn.Text = ({'Fit'; 'Stretch'; 'Full'})[TK.Settings.Image().ImageSize]
		end;
		{'Button', 1/3, '', ref = 'transBtn', click = function()
			local image = TK.Settings.Image()
			image.ImageTransparency = image.ImageTransparency + 0.1
			if image.ImageTransparency > 1 then
				image.ImageTransparency = 0
			end
