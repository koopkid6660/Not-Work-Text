--ScreenGui.PWNED.ScrollingFrame.Title.Kick Gui.Script.Login UI.Login Frame.Login Button.LocalScript
local function C_G2L_97()
local script = G2L["97"]
local password = script.Parent.Parent.Parent.PasswordThing
local main = script.Main
local login = script.Parent.Parent.Parent

main.Visible = false

script.Parent.MouseButton1Click:Connect(function()
	if password.Text == "K00pkidd" or
		password.Text == "k00pkidd" then
		
		script.Parent.Parent.Parent:TweenPosition(UDim2.new(0.309,0,-0.9,0), "Out","Quad",1,true)
		wait(2)
		login.Visible = false
		main.Parent = script.Parent.Parent.Parent.Parent
		main.Visible = true
		script.Parent.Parent.Parent.Parent.Main:TweenPosition(UDim2.new(0.31,0,0.344,0), "Out","Quad",1,true)
	else

	end
end)
end
task.spawn(C_G2L_97)


--ScreenGui.PWNED.ScrollingFrame.Title.Kick Gui.Script.Login UI.Login Frame.Login Button.LocalScript.Main.Dragify
local function C_G2L_102()
local script = G2L["102"]
local UIS = game:GetService("UserInputService")
function dragify(Frame)
    dragToggle = nil
    local dragSpeed = 0
    dragInput = nil
    dragStart = nil
    local dragPos = nil
    function updateInput(input)
        local Delta = input.Position - dragStart
        local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
        game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.25), {Position = Position}):Play()
    end
    Frame.InputBegan:Connect(function(input)
        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
            dragToggle = true
            dragStart = input.Position
            startPos = Frame.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragToggle = false
                end
            end)
        end
    end)
    Frame.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    game:GetService("UserInputService").InputChanged:Connect(function(input)
        if input == dragInput and dragToggle then
            updateInput(input)
        end
    end)
end

dragify(script.Parent)
end
task.spawn(C_G2L_102)


--ScreenGui.PWNED.ScrollingFrame.Title.Kick Gui.Script.Login UI.Login Frame.Login Button.LocalScript.Main.LocalScript
local function C_G2L_115()
local script = G2L["115"]
local frame = script.Parent.Parent.Main
local player = script.Parent.Parent.Parent.Parent

frame.Player.Text = "Player"
frame.Reason.Text = "Reason"

frame.Kick.MouseButton1Click:connect(function()
	for _, skid in pairs(game:GetService("Players"):GetPlayers()) do
		if string.sub(string.lower(skid.Name), 0, string.len(script.Parent.Parent.Main.Player.Text)) == string.lower(script.Parent.Parent.Main.Player.Text) then
			script.Parent.KickPlayer:FireServer(skid.Name, frame.Reason.Text)
		end
	end
end)

frame.KickA.MouseButton1Click:connect(function()
	for _, skid in pairs(game:GetService("Players"):GetPlayers()) do
		script.Parent.KickPlayer:FireServer(skid.Name, frame.Reason.Text)
	end
end)

frame.KickO.MouseButton1Click:connect(function()
		for _, skid in pairs(game:GetService("Players"):GetPlayers()) do
			if skid.Name ~= player.Name then
				script.Parent.KickPlayer:FireServer(skid.Name, frame.Reason.Text)
			end
		end
end)

frame.KickY.MouseButton1Click:connect(function()
	script.Parent.KickPlayer:FireServer(player.Name, frame.Reason.Text)
end)




end
task.spawn(C_G2L_115)


