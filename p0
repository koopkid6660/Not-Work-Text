local bawl = Instance.new("Part", game.Lighting)

bawl.Name="TelekinesisGrab"

local bp = Instance.new("BodyPosition", bawl)

local w = Instance.new("Weld", bawl) 

function onButton1Down(mouse)
	if dojob == true then
	bawl.Size = Vector3.new(2,2,2) 
	
	bawl.Material="Neon"
	
	bawl.Shape=0
	
	bawl.Transparency=trans
	
	bawl.Name="TelekinesisGrab"
	
	bawl.CanCollide = false  
	
	bawl.Parent = game.Workspace
	
	w.Parent = bawl 
	
	w.Name = "Weld"
	
	bp.maxForce = Vector3.new(math.huge,math.huge,math.huge) 
	
	bp.Parent = bawl
	
	bp.Name="lolx"
	
	bawl.Parent = workspace
	
	if mouse ~= nil then
	
		target = mouse.Target()
		if target ~= nil then
			if target.Name ~= "Base" or target.Name ~= "Baseplate" then
			target.Anchored = false
			w.Part0 = target
			
			w.Part1 = bawl 
			
			dwn = true
	
			if target.Parent:FindFirstChild("Humanoid") then
				
				target.Parent:FindFirstChild("Humanoid").PlatformStand=true
				
			else
				
				print('no humanoid')
				
			end
			end
		end
	
	end
	
		
		while dwn == true do 
			
			
				bp.Parent = bawl 
				
				bp.position = Player.Character.HumanoidRootPart.Position + (mouse.Hit().lookVector*dist)
				
				wait(0.05)
			
	    end
	
end

function onButton1Up(mouse)
	if dojob == true then	
		dwn=false

		if bawl ~= nil then
				
			bawl.Parent=game.Lighting 
				
		else 
				
			print('not found') 
				
		end
	end
end 
	
function onKeyDown(key, mouse) 
if dojob == true then		
	key:lower() 
		
	if key == "r" then 
			
		if bawl ~= nil then
				
			--bawl.Parent=nil
				
		end
			
	elseif key == "e" then 
			
		local p = Instance.new("Part", workspace)
			
		p.Size = Vector3.new(3,3,3)
			
		p.Material = "Neon"
			
		p.Position = mouse.Hit().p + Vector3.new(0,2,0) 
			
		p.BrickColor = bawl.BrickColor
			
		p.Shape = "Ball"
			
		p.BottomSurface = "Smooth"
			
		p.TopSurface = "Smooth"
			
	elseif key == "h" then
			
		if bawl ~= nil then
				
			if bawl:FindFirstChild("Weld") ~= nil then
				
				if bawl:FindFirstChild("Weld").Part0 ~= nil then
				
					bawl:FindFirstChild("Weld").Part0.Anchored=false
				
				end
				
			end
				
		end
			
	elseif key == "g" then
			
		if bawl ~= nil then

			local x = Instance.new("Explosion",workspace)

			x.Position=bawl.Position
				
		else
				
			print('some sort of error')
				
		end
			
	elseif key == "x" then 
			
		dist=dist - 5
			
	elseif key == "c" then 
			
		dist=15
			
	elseif key == "v" then 
			
		dist=100
			
	elseif key == "z" then 
			
		dist=dist + 5
			
	end  
		
end
end
		
FireX = 0

FireY = 0

FireZ = 0

SPX = 0

SPY = 0

SPZ = 0

BoxColor = "White"

FireFalse.MouseButton1Down:connect(function ()
	
	local Fire = Instance.new("Fire", bawl)
    Fire.Color = Color3.new(FireX,FireY,FireZ)

    Player.PlayerGui.ScreenGui.FireFalse.Visible = false

    Player.PlayerGui.ScreenGui.FireTrue.Visible = true

    Fire.Color = Color3.new(FireX,FireY,FireZ)
	
end)

