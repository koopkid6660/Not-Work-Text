		end};
		{'TextBox', 2/3, '', ref = 'addPlr'};
		{'ScrollingContainer', 100, ref = 'wlCont'};
		function(hook)
			hook.Whitelist = {}
			local function Unwhitelist(p)
				for x, m in pairs(hook.Whitelist) do
					if m == p then
						table.remove(hook.Whitelist, x)
					end
				end
				TK.Action:FireServer("Server", "RemoveWhitelist", p)						
			end;
			hook.ReorderWL = function()
				local wl = hook.Whitelist
				for i, v in pairs(wlCont.main:GetChildren()) do
					v:Destroy()
				end
				wlCont.drawY = 3
				for i,v in pairs(wl) do
					TK.GUI.Text(wlCont, 2/3, v)
					TK.GUI.Button(wlCont, 1/3, 'Remove').MouseButton1Down:Connect(function()
						Unwhitelist(v)					
						hook.ReorderWL()
					end)
				end
			end
			--hook.ReorderWL()
		end,
	},
