coroutine.wrap(function()
for i = Sound1.Pitch, 0, -0.075 do
Sound1.Pitch = i
wait()
end
end)()
for i = Sound2.Pitch, 0, -0.01 do
Sound2.Pitch = i
wait()
end
Sound1:Stop()
Sound2:Stop()
wait(0.25)
Sound1:Remove()
Sound2:Remove()
end
wait()
end
