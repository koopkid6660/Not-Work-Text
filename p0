Weapon.Parent = Character

Humanoid.Died:connect(function()
	KillChildren(Character)
	HANDS = {}
	HANDFOLDER:ClearAllChildren()
	SCYTHE:ClearAllChildren()
	for i = 1, 100 do
		Swait()
		sick.Volume = sick.Volume - 0.02
	end
end)

