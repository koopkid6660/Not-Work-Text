--ScreenGui.PWNED.ScrollingFrame.Title.black hole.LocalScript
local function C_G2L_468()
local script = G2L["468"]
script.Parent.MouseButton1Click:Connect(function()
	script.Parent.RemoteEvent:FireServer()
end)


end
task.spawn(C_G2L_468)



